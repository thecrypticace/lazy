{
    "name": "thecrypticace/lazy",
    "description": "Collection-like wrapper around Iterators",
    "keywords": ["iterators", "collections", "lazy"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Pittman",
            "email": "jordan@cryptica.me"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "TheCrypticAce\\Lazy\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "symfony/var-dumper": "^4.1",
        "vimeo/psalm": "^3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
